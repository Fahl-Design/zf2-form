{
    "name": "stroker/form",
    "description": "ZF2 module for extending forms with live clientside validation",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "jquery validate",
        "form",
        "validation",
        "zend framework 2",
        "module"
    ],
    "authors": [
        {
            "name": "Bram Gerritsen",
            "email": "bgerritsen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zend-form": "~2.3",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-json": "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-i18n": "~2.3",
        "zendframework/zend-inputfilter": "~2.3",
        "zendframework/zend-servicemanager": "~2.3",
        "zendframework/zend-validator": "~2.3",
        "zendframework/zend-view": "~2.3"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "StrokerForm": "src/",
            "StrokerFormTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
