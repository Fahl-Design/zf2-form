{
    "name": "stroker/form",
    "description": "ZF2 module for extending forms with live clientside validation",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "jquery validate",
        "form",
        "validation",
        "zend framework 2",
        "module"
    ],
    "authors": [
        {
            "name": "Bram Gerritsen",
            "email": "bgerritsen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6 || >= 7.0",
        "zendframework/zendframework": "2.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-0": {
            "StrokerForm": "src/",
            "StrokerFormTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
